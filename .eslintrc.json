{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "globals": {
        "stash": "readonly",
        "PluginApi": "readonly"
    },
    "rules": {
        "quotes": ["warn", "double"],
        "no-console": ["warn", { "allow": ["error", "warn", "debug"] }],
        "no-unused-vars": "warn",
        "no-undef": "warn",
        "camelcase": ["warn"],
        "no-case-declarations": "off",
        "no-prototype-builtins": "off",
        "no-inner-declarations": "off",
        "no-extra-semi": "off",
        "no-empty": "off"
    }
}
